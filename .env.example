# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173,https://admin-controls.drivelah.sg,https://*.onrender.com

# External API URLs
AI_AGENTS_API_URL=https://monitor-api.onrender.com
